<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="14.0" layoutY="64.0" text="Enter your basic city:" textFill="WHITE">
         <font>
            <Font name="Montserrat-Regular" size="12.0" />
         </font></Label>
      <Button layoutX="76.0" layoutY="133.0" mnemonicParsing="false" onAction="#chooseLeftCityAction" text="Apply" />
      <Button layoutX="179.0" layoutY="133.0" mnemonicParsing="false" onAction="#exitModalAction" text="Cancel" />
      <TextField fx:id="leftCityTextField" layoutX="153.0" layoutY="60.0" prefHeight="25.0" prefWidth="125.0" />
      <Label fx:id="errorLabel" layoutX="136.0" layoutY="27.0" />
   </children>
</AnchorPane>
